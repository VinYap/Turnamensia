<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tugasakhir.turnamensiamember.View.Schedule.ScheduleActivity">

    <android.support.design.widget.TabLayout
        android:id="@+id/schedule_tab_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tabGravity="fill"
        app:tabMode="scrollable"></android.support.design.widget.TabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/schedule_pager"
        android:layout_width="match_parent"
        android:layout_height="fill_parent" />
</LinearLayout>
